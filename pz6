right();

if (getColor() == "blue") {
  down(); 
  down(); 
  down();
  if (getColor()== "blue"){
    right(); 
    down();
  }
  if (getColor() == "red"){
    left();
    down();
  }
}


if (getColor() == "red") {
  up(); 
  up(); 
  up();
  if (getColor() == "blue"){
    right(); 
    up();
  }
  if (getColor() == "red"){
    left(); 
    up();
  }
}

