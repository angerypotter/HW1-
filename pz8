function lineDown() {
  while (getColor() == "white") {
   down();
}

  var count = 0;
  while (count < 8) {
    down();
    count = count + 1;
  }
}

function lineUp() {
  while (getColor() == "white") {
   up();
}

  var count = 0;
  while (count < 8) {
    up();
    count = count + 1;
  }
}
    
lineDown()
right()
right()
lineUp()
right()
right()
lineDown()
right()
right()
lineUp()
right()
right()
lineDown()
right()
right()
lineUp()
right()
right()
