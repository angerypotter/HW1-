function lineDown() {
  while (getColor() != "red") {
   down();
}


  if (getColor() == "red") {
   right();
}   else {
     down()
}
}
function lineUp() {
  while (getColor() != "red") {
   up();
}


  if (getColor() == "red") {
   right();
}   else {
     up()
}
}
lineDown()
right()
lineUp()
right()
lineDown()
right()
lineUp()
right()
lineDown()
right()
